APP_ENV=production
APP_NAME=Kanonix API
API_PREFIX=/api/v1
SECRET_KEY=prod-secret-change
ACCESS_TOKEN_EXPIRE_MINUTES=15
REFRESH_TOKEN_EXPIRE_MINUTES=2880
DB_HOST=postgres
DB_PORT=5432
DB_USER=kanonix
DB_PASSWORD=kanonix
DB_NAME=kanonix_prod
REDIS_URL=redis://redis:6379/0
ALLOWED_ORIGINS=["https://api.kanonix.example"]
SWAGGER_ENABLED=false
DEBUG=false
LOG_LEVEL=WARNING
CACHE_TTL_SECONDS=30
RATE_LIMIT_PER_MINUTE=60
WORKERS=4
TENANT_HEADER=X-KANOON-SLUG
STORAGE_MODE=minio
MINIO_ENDPOINT=http://minio:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin
MINIO_BUCKET=kanonix-gallery
MINIO_REGION=us-east-1
USE_SSL=true
