APP_ENV=staging
APP_NAME=Kanonix API
API_PREFIX=/api/v1
SECRET_KEY=staging-secret-change
ACCESS_TOKEN_EXPIRE_MINUTES=20
REFRESH_TOKEN_EXPIRE_MINUTES=4320
DB_HOST=postgres
DB_PORT=5432
DB_USER=kanonix
DB_PASSWORD=kanonix
DB_NAME=kanonix_staging
REDIS_URL=redis://redis:6379/0
ALLOWED_ORIGINS=["https://staging.kanonix.local"]
SWAGGER_ENABLED=true
DEBUG=false
LOG_LEVEL=INFO
CACHE_TTL_SECONDS=45
RATE_LIMIT_PER_MINUTE=90
WORKERS=3
TENANT_HEADER=X-KANOON-SLUG
STORAGE_MODE=minio
MINIO_ENDPOINT=http://minio:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin
MINIO_BUCKET=kanonix-gallery
MINIO_REGION=us-east-1
USE_SSL=false
